{
  "name": "@kunukn/vue-2-datepicker",
  "private": false,
  "version": "0.0.5",
  "type": "module",
  "main": "./dist/vue-datepicker.umd.cjs",
  "module": "./dist/vue-datepicker.js",
  "exports": {
    ".": {
      "import": "./dist/vue-datepicker.js",
      "require": "./dist/vue-datepicker.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunukn/vue-2-datepicker.git"
  },
  "bugs": {
    "url": "https://github.com/kunukn/vue-2-datepicker/issues"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "vue-tsc && vite build",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:css": "stylelint \"{assets/styles/**/*.scss,components/**/*.vue/**/*.vue}\" --config .stylelintrc.js",
    "lint:css-cache": "stylelint \"{components/**/*.vue/**/*.vue}\" --config .stylelintrc.js --cache",
    "test": "vitest --environment jsdom --root src/"
  },
  "license": "MIT",
  "files": [
    "dist/*",
    "src/locale/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "cross-env": "^7.0.3",
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.16.18",
    "@vitest/coverage-c8": "^0.32.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^1.3.6",
    "@vue/tsconfig": "^0.1.3",
    "@vue/vue2-jest": "^29.2.4",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit": "^16.0.0",
    "jest-transform-stub": "^2.0.0",
    "jest-trx-results-processor": "^3.0.2",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.63.4",
    "sass-loader": "^10.4.1",
    "ts-jest": "^29.1.0",
    "ts-loader": "^8.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.3.9",
    "vite-plugin-vue2": "^2.0.3",
    "vitest": "^0.32.2",
    "vue": "2.6.12",
    "vue-jest": "^3.0.7",
    "vue-server-renderer": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "vue-tsc": "^1.8.1"
  },
  "peerDependencies": {
    "vue": "^2.x.x"
  },
  "resolutions": {
    "vue-template-compiler": "2.6.12"
  }
}

{
  "name": "vue-2-datepicker",
  "version": "0.0.1",
  "author": "kunukn",
  "license": "MIT",
  "description": "A simple Vue 2 datepicker component. Supports disabling of dates, inline mode, translations",
  "main": "dist/vue-2-datepicker.ssr.js",
  "browser": "dist/vue-2-datepicker.esm.js",
  "module": "dist/vue-2-datepicker.esm.js",
  "unpkg": "dist/vue-2-datepicker.min.js",
  "types": "vue-2-datepicker.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunukn/vue-2-datepicker.git"
  },
  "bugs": {
    "url": "git+https://github.com/kunukn/vue-2-datepicker/issues"
  },
  "files": [
    "dist/*",
    "vue-2-datepicker.d.ts"
  ],
  "keywords": [
    "vue 2",
    "datepicker",
    "date-picker"
  ],
  "sideEffects": false,
  "scripts": {
    "//": "openssl-legacy-provider is for those using NodeJS 18+ and being backward compatible",
    "serve": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service serve dev/serve.ts --port 8085 --open",
    "dev": "yarn serve",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "test": "jest --config test/unit/jest.conf.js --coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/node": "^20.3.1",
    "@vue/babel-preset-app": "^5.0.8",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit": "^16.0.0",
    "jest-transform-stub": "^2.0.0",
    "jest-trx-results-processor": "^3.0.2",
    "minimist": "^1.2.8",
    "prettier": "^2.8.8",
    "rollup": "^3.25.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.9.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "typescript": "^5.1.3",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.7.14"
  },
  "peerDependencies": {
    "vue": "2.6.x"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  }
}
